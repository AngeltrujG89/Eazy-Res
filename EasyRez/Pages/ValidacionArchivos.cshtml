@page
@model EasyRez.Pages.ValidacionArchivosModel
@{

}

<div class="col-8 align-self-start">
    <h2>Sistema Prueba de Easy-Rez</h2>
</div>
<hr />

<div class="contenedor">

    <div class="row justify-content-end">
        <div class="col-10">
            <input type="file" id="archivo">
            <button class="btn btn-primary" onclick="ValidarArchivo">Cargar</button>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-12">
            <span id="textHeader"></span>
            <h5>LOG</h5>
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Linea</th>
                        <th>Descripción</th>
                        <th>Nombre del archivo</th>
                        <th>Tipo</th>
                        <th>Fecha y hora</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>